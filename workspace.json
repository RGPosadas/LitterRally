{
  "version": 1,
  "projects": {
    "rally-ui": {
      "root": "apps/rally-ui",
      "sourceRoot": "apps/rally-ui/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/rally-ui",
            "index": "apps/rally-ui/src/index.html",
            "main": "apps/rally-ui/src/main.tsx",
            "polyfills": "apps/rally-ui/src/polyfills.ts",
            "tsConfig": "apps/rally-ui/tsconfig.app.json",
            "assets": [
              "apps/rally-ui/src/favicon.ico",
              "apps/rally-ui/src/manifest.json",
              "apps/rally-ui/src/assets"
            ],
            "styles": ["apps/rally-ui/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/rally-ui/src/environments/environment.ts",
                  "with": "apps/rally-ui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "rally-ui:build",
            "proxyConfig": "apps/rally-ui/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "rally-ui:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/rally-ui/tsconfig.app.json",
              "apps/rally-ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/rally-ui/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/rally-ui/jest.config.js",
            "tsConfig": "apps/rally-ui/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "rally-ui-e2e": {
      "root": "apps/rally-ui-e2e",
      "sourceRoot": "apps/rally-ui-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/rally-ui-e2e/cypress.json",
            "tsConfig": "apps/rally-ui-e2e/tsconfig.e2e.json",
            "devServerTarget": "rally-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "rally-ui:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/rally-ui-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/rally-ui-e2e/**/*"]
          }
        }
      }
    },
    "rally-api": {
      "root": "apps/rally-api",
      "sourceRoot": "apps/rally-api/src",
      "projectType": "application",
      "prefix": "rally-api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/rally-api",
            "main": "apps/rally-api/src/main.ts",
            "tsConfig": "apps/rally-api/tsconfig.app.json",
            "assets": ["apps/rally-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/rally-api/src/environments/environment.ts",
                  "with": "apps/rally-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "rally-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/rally-api/tsconfig.app.json",
              "apps/rally-api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/rally-api/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/rally-api/jest.config.js",
            "tsConfig": "apps/rally-api/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "types": {
      "root": "libs/types",
      "sourceRoot": "libs/types/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/types/tsconfig.lib.json",
              "libs/types/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/types/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/types/jest.config.js",
            "tsConfig": "libs/types/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "rally-ui"
}

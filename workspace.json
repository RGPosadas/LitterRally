{
  "version": 1,
  "projects": {
    "mull-ui": {
      "root": "apps/mull-ui",
      "sourceRoot": "apps/mull-ui/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "stylePreprocessorOptions": {
              "includePaths": ["libs/ui-lib/src/lib/styles"]
            },
            "extractCss": true,
            "outputPath": "dist/apps/mull-ui",
            "index": "apps/mull-ui/src/index.html",
            "main": "apps/mull-ui/src/main.tsx",
            "polyfills": "apps/mull-ui/src/polyfills.ts",
            "tsConfig": "apps/mull-ui/tsconfig.app.json",
            "assets": [
              "apps/mull-ui/src/favicon.ico",
              "apps/mull-ui/src/manifest.json",
              "apps/mull-ui/src/assets"
            ],
            "styles": ["libs/ui-lib/src/lib/styles/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/mull-ui/src/environments/environment.ts",
                  "with": "apps/mull-ui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "mull-ui:build",
            "proxyConfig": "apps/mull-ui/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "mull-ui:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/mull-ui/tsconfig.app.json", "apps/mull-ui/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/mull-ui/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/mull-ui/jest.config.js",
            "tsConfig": "apps/mull-ui/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "mull-ui-e2e": {
      "root": "apps/mull-ui-e2e",
      "sourceRoot": "apps/mull-ui-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/mull-ui-e2e/cypress.json",
            "tsConfig": "apps/mull-ui-e2e/tsconfig.e2e.json",
            "devServerTarget": "mull-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mull-ui:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/mull-ui-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/mull-ui-e2e/**/*"]
          }
        }
      }
    },
    "mull-api": {
      "root": "apps/mull-api",
      "sourceRoot": "apps/mull-api/src",
      "projectType": "application",
      "prefix": "mull-api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/mull-api",
            "main": "apps/mull-api/src/main.ts",
            "tsConfig": "apps/mull-api/tsconfig.app.json",
            "assets": ["apps/mull-api/src/assets"],
            "showCircularDependencies": false
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/mull-api/src/environments/environment.ts",
                  "with": "apps/mull-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "mull-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/mull-api/tsconfig.app.json", "apps/mull-api/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/mull-api/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/mull-api/jest.config.js",
            "tsConfig": "apps/mull-api/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "types": {
      "root": "libs/types",
      "sourceRoot": "libs/types/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["libs/types/tsconfig.lib.json", "libs/types/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!libs/types/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/types/jest.config.js",
            "tsConfig": "libs/types/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "ui-lib": {
      "root": "libs/ui-lib",
      "sourceRoot": "libs/ui-lib/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["libs/ui-lib/tsconfig.lib.json", "libs/ui-lib/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!libs/ui-lib/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/ui-lib/jest.config.js",
            "tsConfig": "libs/ui-lib/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "mull-ui"
}
